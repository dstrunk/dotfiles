if exists('*IPhpInsertUse')
  finish
endif

function! IPhpInsertUse()
  call PhpInsertUse()
  call feedkeys('a', 'n')
endfunction
noremap <Leader>n :call PhpInsertUse()<CR>

if exists('*IPhpExpandClass')
